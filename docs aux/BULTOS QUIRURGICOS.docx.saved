<cell xid="495" props="bot-style:1; bot-thickness:3.000000pt; right-attach:1; left-thickness:3.000000pt; right-thickness:3.000000pt; left-attach:0; top-attach:0; right-style:1; table-column-props:44.500000pt/73.500000pt/228.700000pt/54.850000pt/53.850000pt/; left-style:1; top-thickness:3.000000pt; bot-attach:1; top-style:1">
<p xid="496" props="text-align:center">Partida</p>
</cell>
<cell xid="497" props="bot-style:1; bot-thickness:3.000000pt; right-attach:2; left-thickness:3.000000pt; right-thickness:3.000000pt; left-attach:1; top-attach:0; right-style:1; table-column-props:44.500000pt/73.500000pt/228.700000pt/54.850000pt/53.850000pt/; left-style:1; top-thickness:3.000000pt; bot-attach:1; top-style:1">
<p xid="498" props="text-align:center">Clave</p>
